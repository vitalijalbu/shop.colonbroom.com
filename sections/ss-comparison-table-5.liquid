  assign property_heading_height = section.settings.property_heading_height
  assign property_heading_align = section.settings.property_heading_align
  assign property_heading_align_mobile = section.settings.property_heading_align_mobile

  assign property_text_size = section.settings.property_text_size
  assign property_text_size_mobile = section.settings.property_text_size_mobile
  assign property_text_color = section.settings.property_text_color
  assign property_text_custom = section.settings.property_text_custom
  assign property_text_font = section.settings.property_text_font
  assign property_text_height = section.settings.property_text_height
  assign property_text_align = section.settings.property_text_align
  assign property_text_align_mobile = section.settings.property_text_align_mobile

  assign button = section.settings.button
  assign button_url = section.settings.button_url
